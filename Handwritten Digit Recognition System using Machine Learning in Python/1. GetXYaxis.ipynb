   "source": [
    "from pynput.mouse import Listener\n",
    "\n",
    "def on_click(x, y, button, pressed):\n",
    "    if pressed:\n",
    "        print(f\"Clicked at ({x}, {y})\")\n",
    "\n",
    "with Listener(on_click=on_click) as listener:\n",
    "    listener.join()\n"
   ]
